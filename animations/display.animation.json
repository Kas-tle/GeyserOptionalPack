{
	"format_version": "1.8.0",
	"animations": {
		"animation.display.interpolate": {
            "override_previous_animation": true,
			"loop": true,
			"bones": {
				"geyser":{
					"scale": [
                        "math.lerp(v.scale.x, q.actor_property('geyser:scale_x'), v.interpolation_state)",
                        "math.lerp(v.scale.y, q.actor_property('geyser:scale_y'), v.interpolation_state)",
                        "math.lerp(v.scale.z, q.actor_property('geyser:scale_z'), v.interpolation_state)"
                    ],
                    "position": [
                        "math.lerp(v.translation.x, q.actor_property('geyser:translation_x'), v.interpolation_state)",
                        "math.lerp(v.translation.y, q.actor_property('geyser:translation_y'), v.interpolation_state)",
                        "math.lerp(v.translation.z, q.actor_property('geyser:translation_z'), v.interpolation_state)"
                    ]
				},
                "geyser_x": {
                    "rotation": [
                        "math.lerprotate(v.rotation.x, q.actor_property('geyser:rotation_x'), v.interpolation_state)",
                        0,
                        0
                    ]
                },
                "geyser_y": {
                    "rotation": [
                        0,
                        "math.lerprotate(v.rotation.y, q.actor_property('geyser:rotation_y'), v.interpolation_state)",
                        0
                    ]
                },
                "geyser_z": {
                    "rotation": [
                        0,
                        0,
                        "math.lerprotate(v.rotation.z, q.actor_property('geyser:rotation_z'), v.interpolation_state)"
                    ]
                }
			}
		},
        "animation.display.stable": {
            "override_previous_animation": true,
			"loop": true,
			"bones": {
				"geyser":{
					"scale": [
                        "q.actor_property('geyser:scale_x')",
                        "q.actor_property('geyser:scale_y')",
                        "q.actor_property('geyser:scale_z')"
                    ],
                    "position": [
                        "q.actor_property('geyser:translation_x')",
                        "q.actor_property('geyser:translation_y')",
                        "q.actor_property('geyser:translation_z')"
                    ]
				},
                "geyser_x": {
                    "rotation": [
                        "q.actor_property('geyser:rotation_x')",
                        0,
                        0
                    ]
                },
                "geyser_y": {
                    "rotation": [
                        0,
                        "q.actor_property('geyser:rotation_y')",
                        0
                    ]
                },
                "geyser_z": {
                    "rotation": [
                        0,
                        0,
                        "q.actor_property('geyser:rotation_z')"
                    ]
                }
			}
		}
	}
}
